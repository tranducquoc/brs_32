<%- if controller_name != "sessions" %>
  <%= link_to t("devise.shared.login_link"), new_session_path(resource_name) %>
  <br />
<% end -%>
<%- if devise_mapping.registerable? && controller_name != "registrations" %>
  <%= link_to t("devise.shared.signup_link"),
    new_registration_path(resource_name) %><br />
<% end -%>
<%- if devise_mapping.recoverable? && controller_name != "passwords" &&
  controller_name != "registrations" %>
  <%= link_to t("devise.shared.forgot_passowrd"),
    new_password_path(resource_name) %><br />
<% end -%>
<%- if devise_mapping.confirmable? && controller_name != "confirmations" %>
  <%= link_to t("devise.shared.not_recieve_email"),
    new_confirmation_path(resource_name) %>
  <br />
<% end -%>
<%- if devise_mapping.lockable? &&
  resource_class.unlock_strategy_enabled?(:email) &&
  controller_name != "unlocks" %>
  <%= link_to t("devise.shared.not_unlock"),
    new_unlock_path(resource_name) %><br />
<% end -%>
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
      <%= link_to omniauth_authorize_path(resource_name, provider) do %>
        <%= button_tag "", class: "btn_social btn_facebook" %>
      <% end %>
    <br />
  <% end -%>
  <%= link_to user_google_oauth2_omniauth_authorize_path do %>
    <%= button_tag "", class: "btn_social btn_google" %>
  <%= link_to user_twitter_omniauth_authorize_path do %>
    <%= button_tag "", class: "btn_social btn_twitter" %>
  <% end %>
<% end -%>
